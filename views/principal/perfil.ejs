    <% } %>

    <% produtos.forEach(function(produto){%>
        <hr>
        <h3> <%= produto.nome %> </h3>
        <h4> <%= produto.preco %> </h4>
        
    <%  })  %>

    <img src="/img/img01.jpg" alt="">

